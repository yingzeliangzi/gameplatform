# 环境
NODE_ENV=development

# 接口前缀
VUE_APP_BASE_API=/api

# 代理目标
VUE_APP_TARGET=http://localhost:8080

# WebSocket地址
VUE_APP_WS_URL=ws://localhost:8080/ws

# 开发服务器端口
PORT=8081

# 文件上传限制
VUE_APP_UPLOAD_MAX_SIZE=10485760

# JWT存储键名
VUE_APP_TOKEN_KEY=game_platform_token

# 加密密钥
VUE_APP_CRYPTO_KEY=your-crypto-key